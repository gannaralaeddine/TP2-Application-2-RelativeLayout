<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/natalie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lylame"
        android:textSize="30sp"
        android:text="@string/natalie"/>

    <TextView
        android:id="@+id/kunalben"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/natalie"
        android:textSize="30sp"
        android:text="@string/kunalben"/>

    <TextView
        android:id="@+id/kagure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/kunalben"
        android:textSize="30sp"
        android:text="@string/kagure"/>

    <TextView
        android:id="@+id/lylame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:textSize="30sp"
        android:text="@string/lylame"/>

    <TextView
        android:id="@+id/omoju"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/anyjennie"
        android:layout_alignParentEnd="true"
        android:textSize="30sp"
        android:text="@string/omoju"/>

    <TextView
        android:id="@+id/anyjennie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:textSize="30sp"
        android:text="@string/anyjennie"/>

    <TextView
        android:id="@+id/amy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/omoju"
        android:layout_alignParentEnd="true"
        android:textSize="30sp"
        android:text="@string/amy"/>

</RelativeLayout>